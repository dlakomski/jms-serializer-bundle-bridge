{
    "name": "dlakomski/jms-serializer-bundle-bridge",
    "description": "Symfony bundle for using JMSSerializer as message serializer for SimpleBus/Asynchronous",
    "keywords": [
        "asynchronous",
        "message",
        "serializer"
    ],
    "homepage": "http://github.com/SimpleBus/JMSSerializerBundleBridge",
    "license": "MIT",
    "authors": [
        {
            "name": "Cliff Odijk",
            "email": "cliff@jcid.nl"
        },
        {
            "name": "Ruud Kamphuis",
            "homepage": "https://github.com/ruudk"
        },
        {
            "name": "Matthias Noback",
            "email": "matthiasnoback@gmail.com",
            "homepage": "http://php-and-symfony.matthiasnoback.nl"
        }
    ],
    "type": "symfony-bundle",
    "require": {
        "php": "^8.2",
        "dlakomski/asynchronous-bundle": "^6.4",
        "dlakomski/jms-serializer-bridge": "^6.4",
        "jms/serializer-bundle": "^5.5",
        "symfony/config": "^6.4.23 || ^7.4.1",
        "symfony/dependency-injection": "^6.4.23 || ^7.4",
        "symfony/framework-bundle": "^6.4.23 || ^7.4"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.11",
        "phpunit/phpunit": "^11.5.46",
        "symfony/phpunit-bridge": "^6.4.23 || ^7.4"
    },
    "autoload": {
        "psr-4": {
            "SimpleBus\\JMSSerializerBundleBridge\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SimpleBus\\JMSSerializerBundleBridge\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        }
    },
    "conflict": {
        "doctrine/annotations": "<1.10.4",
        "jms/metadata": "<2.5.2",
        "jms/serializer": "<3.2.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
